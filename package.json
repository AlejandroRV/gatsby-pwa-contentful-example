{
  "name": "gatsby-pwa-contentful-example",
  "private": true,
  "description": "Example of Gatsby.js as a Progressive web app that pulls data from Contentful",
  "version": "0.1.0",
  "license": "MIT",
  "scripts": {
    "build": "gatsby build",
    "develop": "env-cmd -f .env gatsby develop",
    "format": "prettier --write \"**/*.{js,jsx,json,md}\"",
    "start": "npm run develop",
    "serve": "gatsby serve",
    "clean": "gatsby clean",
    "test": "echo \"Write tests! -> https://gatsby.dev/unit-testing\" && exit 1"
  },
  "dependencies": {
    "@contentful/rich-text-react-renderer": "^13.4.0",
    "@material-ui/core": "^4.7.0",
    "@material-ui/icons": "^4.5.1",
    "env-cmd": "^10.0.1",
    "eslint-config-react-app": "^5.0.2",
    "gatsby": "^2.17.11",
    "gatsby-plugin-manifest": "^2.2.30",
    "gatsby-plugin-offline": "^3.0.24",
    "gatsby-plugin-react-helmet": "^3.1.15",
    "gatsby-plugin-sass": "^2.1.23",
    "gatsby-plugin-sharp": "^2.3.1",
    "gatsby-source-filesystem": "^2.1.36",
    "gatsby-source-contentful": "2.1.19",
    "gatsby-theme-material-ui": "^1.0.5",
    "gatsby-transformer-remark": "^5.25.1",
    "node-sass": "^4.13.1",
    "react": "^16.11.0",
    "react-dom": "^16.11.0",
    "react-helmet": "^5.2.1"
  },
  "devDependencies": {
    "prettier": "^1.19.1",
    "env-cmd": "^10.0.1"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlejandroRV/gatsby-pwa-contentful-example"
  },
  "bugs": {
    "url": "https://github.com/AlejandroRV/gatsby-pwa-contentful-example/issues"
  }
}
